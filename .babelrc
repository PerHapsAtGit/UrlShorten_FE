{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions"
                    ]
                },
                "shippedProposals": true
            }
        ],
        "@babel/preset-react"
    ],
    "env": {
        "test": {
            "plugins": ["istanbul"]
        }
    },
    "plugins": [
        [ "babel-plugin-webpack-aliases", { "config": "./webpack.config.js" } ] 
    ]
}